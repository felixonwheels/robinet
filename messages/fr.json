{
	"$schema": "https://inlang.com/schema/inlang-message-format",
	"hello_world": "Hello, {name} from fr!",
	"dragAndDrop": "glissez-déposez votre fichier GPX ici, ou cliquez pour le sélectionner",
	"fileRestriction": "vous ne pouvez télécharger qu'un seul fichier",
	"filePoints": [
		{
			"declarations": ["input count", "local countPlural = count: plural"],
			"selectors": ["countPlural"],
			"match": {
				"countPlural=one": "le fichier contient 1 point",
				"countPlural=other": "le fichier contient {count} points"
			}
		}
	],
	"uploadErrorMaxFileSize": "taille maximale du fichier dépassée",
	"uploadErrorMaxFilesUploaded": "nombre maximal de fichiers téléchargés",
	"uploadErrorFileTypeNotAllowed": "type de fichier non autorisé",
	"uploadErrorFailToUpload": "échec du téléchargement pour {name} !",
	"titleLead": "ajoutez les sources d'eau autour de votre tracé GPX et restez hydratés",
	"bufferSize": "taille du boyau",
	"waterSourceCount": [
		{
			"declarations": ["input count", "local countPlural = count: plural"],
			"selectors": ["countPlural"],
			"match": {
				"countPlural=one": "point d'eau ajouté",
				"countPlural=other": "points d'eau ajoutés"
			}
		}
	],
	"downloadNewGpx": [
		{
			"declarations": ["input count", "input countPlural"],
			"selectors": ["countPlural"],
			"match": ["télécharger le nouveau fichier GPX"]
		}
	]
}
